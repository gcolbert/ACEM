<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces
                      http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">

	<url-mapping id="home">
		<pattern value="/home" />
		<view-id value="/pages/home.xhtml" />
	</url-mapping>

	<url-mapping id="pedagogical-advice">
		<pattern value="/pedagogical-advice" />
		<view-id value="/pages/pedagogical-advice.xhtml" />
	</url-mapping>

	<url-mapping id="my-tools">
		<pattern value="/my-tools" />
		<view-id value="/pages/my-tools.xhtml" />
	</url-mapping>

	<url-mapping id="my-tools-param">
		<pattern value="/my-tools?toolCategoryId=#{toolCategoryId}" />
		<view-id value="/pages/my-tools.xhtml" />
	</url-mapping>

	<url-mapping id="my-services">
		<pattern value="/my-services" />
		<view-id value="/pages/my-services.xhtml" />
	</url-mapping>

	<url-mapping id="my-scenarios">
		<pattern value="/my-scenarios" />
		<view-id value="/pages/my-scenarios.xhtml" />
	</url-mapping>
<!-- 
	<url-mapping id="my-scenarios-param">
		<pattern value="/my-scenarios/#{scenarioId}" />
		<view-id value="/pages/my-scenarios.xhtml" />
	</url-mapping>
 -->
	<url-mapping id="my-teachings">
		<pattern value="/my-teachings" />
		<view-id value="/pages/my-teachings.xhtml" />
	</url-mapping>

	<url-mapping id="scenarios-library">
		<pattern value="/scenarios-library" />
		<view-id value="/pages/scenarios-library.xhtml" />
	</url-mapping>

	<url-mapping id="administration">
		<pattern value="/administration" />
		<view-id value="/pages/administration.xhtml" />
	</url-mapping>

	<url-mapping id="timeout">
		<pattern value="/timeout" />
		<view-id value="/pages/timeout.xhtml" />
	</url-mapping>

	<url-mapping id="logout">
		<pattern value="/logout" />
		<view-id value="/pages/logout.xhtml" />
	</url-mapping>

	<url-mapping id="exception">
		<pattern value="/exception" />
		<view-id value="/pages/exceptions/exception.xhtml" />
	</url-mapping>

</pretty-config>